<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTextmain" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/main_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="AutoCompleteTextViewmain" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/main_list_selector_holo_light</item>
	  <item name="android:background">@drawable/main_edit_text_holo_light</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="CheckBoxmain" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/main_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonmain" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/main_btn_radio_holo_light</item>
  </style>

  <style name="Buttonmain" parent="android:Widget.Button">
	  <item name="android:background">@drawable/main_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ImageButtonmain" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/main_btn_default_holo_light</item>
  </style>

  <style name="Spinnermain" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/main_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/main_list_selector_holo_light</item>
  </style>

  <style name="SpinnerDropDownItemmain" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/main_btn_radio_holo_light</item>
  </style>
  <style name="Tabmain">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/main_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

	<style name="TabTextmain" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">16sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <!--item name="android:maxWidth">180dip</item-->
  </style>

  <style name="ProgressBarmain" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/main_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/main_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarmain" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/main_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/main_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/main_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="RatingBarmain" parent="android:Widget.RatingBar">
      <item name="android:progressDrawable">@drawable/main_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/main_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigmain">
      <item name="android:progressDrawable">@drawable/main_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/main_ratingbar_holo_light</item>
      <item name="android:minHeight">35dip</item>
      <item name="android:maxHeight">35dip</item>
  </style>

  <style name="RatingBarSmallmain">
      <item name="android:progressDrawable">@drawable/main_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/main_ratingbar_small_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="Togglemain" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/main_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ListViewmain" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/main_list_selector_holo_light</item>
  </style>

  <style name="ListViewmain.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/main_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemmain" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#000000</item>
  </style>

</resources>
